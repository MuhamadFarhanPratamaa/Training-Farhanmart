<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- farhanmart.person tree view -->
    <record id="farhanmart_person_view_tree" model="ir.ui.view">
        <field name="name">farhanmart.person.view.tree</field>
        <field name="model">farhanmart.person</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="alamat"/>
                <field name="tgl_lahir"/>
                
            </tree>
        </field>
    </record>

    <!-- farhanmart.person form view -->
    <record id="farhanmart_person_view_form" model="ir.ui.view">
        <field name="name">farhanmart.person.view.form</field>
        <field name="model">farhanmart.person</field>
        <field name="arch" type="xml">
            <form string="Nama">
                <sheet>
                    <group name="name" string="Nama">
                    <field name="name"/>
                     </group>

                     <group name="data" string="Data">
                 
                     <field name="alamat"/>
                     <field name="tgl_lahir"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
